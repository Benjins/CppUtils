int IsPrime(int val){
	int counter = 2;
	int factorCount = 0;
	while(counter < val){
		if ((val / counter) * counter == val){
			factorCount = factorCount + 1;
		}

		counter = counter + 1;
	}

	return factorCount == 0;
}

void main(){
	int val = READI();
	while (val > 0){
		if (IsPrime(val)){
			PRINTI(val);
		}
		
		val = READI();
	}
}